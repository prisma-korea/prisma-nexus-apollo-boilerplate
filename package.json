{
  "name": "prisma-nexus-apollo-boilerplate",
  "packageManager": "yarn@3.2.1",
  "version": "1.0.0",
  "description": "Typescript apollo prisma boilerplate",
  "repository": "https://github.com/prisma-korea/prisma-nexus-apollo-boilerplate",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e ./dotenv/dev.env -- ts-node --transpile-only --files src/server.ts",
    "dev": "dotenv -e ./dotenv/dev.env -- ts-node-dev --respawn --transpile-only --files src/server.ts",
    "prod": "node dist/server",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "tsc": "tsc",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand",
    "test:watch": "jest --watch",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "migrate:local": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "migrate": "dotenv -e ./dotenv/dev.env -- prisma migrate deploy",
    "migrate:dev": "dotenv -e ./dotenv/dev.env -- prisma migrate dev",
    "migrate:reset": "dotenv -e ./dotenv/dev.env -- prisma migrate reset",
    "migrate:deploy": "dotenv -e ./dotenv/dev.env -- prisma migrate deploy",
    "db-push": "dotenv -e ./dotenv/dev.env -- prisma db push",
    "db-push:test": "dotenv -e ./dotenv/test.env -- prisma db push",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@graphql-tools/schema": "^9.0.4",
    "@prisma/client": "4.4.0",
    "@types/ejs": "^3.1.1",
    "altair-express-middleware": "^5.0.0",
    "apollo-server": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "ejs": "^3.1.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.1.33",
    "graphql-shield": "^7.6.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^15.0.2",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.3.0",
    "nexus-validation-plugin": "^0.2.0",
    "path": "^0.12.7",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@dooboo/eslint-config-node": "^1.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "^4.17.14",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "^18.8.4",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.25.0",
    "graphql-import": "^1.0.2",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "jest": "29.1.2",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^15.0.2"
  }
}
