{
  "name": "ts-apollo-prisma",
  "version": "1.0.0",
  "description": "Typescript apollo prisma boilerplate",
  "repository": "https://github.com/dooboolab/ts-apollo-prisma",
  "license": "MIT",
  "scripts": {
    "local": "dotenv -e ./dotenv/dev.env -- ts-node-dev --respawn --transpile-only --files src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "ts-node src/server.ts",
    "prod": "node dist/server",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src tests",
    "tsc": "tsc",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "test": "dotenv -e ./dotenv/test.env -- jest --runInBand",
    "test:watch": "jest --watch",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "migrate:local": "dotenv -e ./dotenv/.env -- prisma migrate dev --preview-feature",
    "migrate": "dotenv -e ./dotenv/dev.env -- prisma migrate deploy --preview-feature",
    "migrate:dev": "dotenv -e ./dotenv/dev.env -- prisma migrate dev --preview-feature",
    "migrate:reset": "dotenv -e ./dotenv/dev.env -- prisma migrate reset --preview-feature",
    "migrate:deploy": "dotenv -e ./dotenv/dev.env -- prisma migrate deploy --preview-feature",
    "db-push": "dotenv -e ./dotenv/dev.env -- prisma db push --preview-feature",
    "db-push:test": "dotenv -e ./dotenv/test.env -- prisma db push --preview-feature",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "2.18.0",
    "apollo-server": "^2.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^6.0.4",
    "graphql-shield": "^7.5.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.32.0"
  },
  "devDependencies": {
    "@dooboo/eslint-config": "^0.6.23",
    "@prisma/cli": "~2.18.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "^4.17.11",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "^14.14.34",
    "@types/ws": "7.4.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-ws": "^1.0.20",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "graphql-import": "^1.0.2",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "jest": "26.6.3",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
